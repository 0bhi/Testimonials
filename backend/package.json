    "typescript": "^5.7.3",
    "wrangler": "^4.32.0"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "bcryptjs": "^2.4.3",
    "drizzle-orm": "^0.29.3",
